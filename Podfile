# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Gather' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!
# 网络请求
   pod 'AFNetworking'
# 图片缓存
   pod 'SDWebImage'
# 集成腾讯bugly，用于打包测试和bug异常收集
   pod 'Bugly'
# 富文本
pod 'YYText'
# 字典转模型
pod 'YYModel'
# YYImage
pod 'YYImage'
# webP
pod 'YYImage/WebP'
# 高性能的 iOS 缓存框架
pod 'YYCache'
# 高性能的 iOS 异步图像加载框架。
pod 'YYWebImage'
# iOS 键盘监听管理工具
pod 'YYKeyboardManager'
# HUD
pod 'MBProgressHUD'
# AutoLayout
pod 'Masonry'
# 二维码，条形码
pod 'LBXScan/LBXNative'
pod 'LBXScan/LBXZXing'
pod 'LBXScan/LBXZBar'
pod 'LBXScan/UI'
# 根据颜色 生成一张图片
pod 'UIImage+ImageWithColor'
# 在开发时在iOS应用程序中查找内存泄漏
pod 'MLeaksFinder'
# 轮播图
pod'SDCycleScrollView'
# 类似网易新闻首页
pod 'WMPageController'
  # Pods for Gather

  target 'GatherUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
